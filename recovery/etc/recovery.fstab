/cache            ext4       /dev/block/bootdevice/by-name/cache
/system           ext4       /dev/block/bootdevice/by-name/system
/data             ext4       /dev/block/bootdevice/by-name/userdata   length=-16384
/boot             emmc       /dev/block/bootdevice/by-name/boot
/recovery         emmc       /dev/block/bootdevice/by-name/recovery

/sdcard            vfat      /dev/block/mmcblk0p29 		flags=display="Internal SD";storage;wipeingui;removable
/external_sd       vfat      /dev/block/mmcblk1p1 		flags=display="External SD";storage;wipeingui;removable
/usbdisk	       vfat		 /dev/block/sda1	/dev/block/sda	flags=display="USB OTG";storage;removable
