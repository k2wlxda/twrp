/cache            ext4       /dev/block/bootdevice/by-name/cache
/system           ext4       /dev/block/bootdevice/by-name/system
/data             ext4       /dev/block/bootdevice/by-name/userdata   length=-16384
/boot             emmc       /dev/block/bootdevice/by-name/boot
/recovery         emmc       /dev/block/bootdevice/by-name/recovery

/sdcard            vfat      /devices/soc.0/7824900.sdhci/mmc_host 		flags=display="Sdcard";storage;wipeingui;removable
/external_sd       vfat      /devices/soc.0/7864900.sdhci/mmc_host 		flags=display="External SD";storage;wipeingui;removable
/usbdisk	       vfat		 /devices/platform/msm_hsusb_host/usb1	    flags=display="USB OTG";storage;removable
